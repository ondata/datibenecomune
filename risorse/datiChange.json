  "document_id": "25684771",
  "ask": "COVID-19: i dati su cui prende le decisioni il governo devono essere pubblici!",
  "created_at": "2020-11-05T16:16:00Z",
  "deleted_at": null,
  "published_at": "2020-11-05T17:14:25Z",
  "last_edited_at": "2021-06-03T09:43:47Z",
  "original_locale": "it-IT",
